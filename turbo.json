{
	"$schema": "https://turbo.build/schema.json",
	"globalDotEnv": [".env"],
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".svelte-kit/**", ".vercel/**"]
		},
		"lint": {},
		"test:unit": {},
		"package": {
			"dependsOn": ["^build"]
		},
		"release": {
			"dependsOn": ["^package"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
